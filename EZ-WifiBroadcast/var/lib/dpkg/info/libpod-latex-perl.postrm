#!/bin/sh

set -e

if [ "$1" = remove ]
then
    dpkg-divert --remove --package libpod-latex-perl --rename \
        --divert /usr/bin/pod2latex.bundled /usr/bin/pod2latex

    dpkg-divert --remove --package libpod-latex-perl --rename \
        --divert /usr/share/man/man1/pod2latex.bundled.1.gz /usr/share/man/man1/pod2latex.1.gz
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0
